        "package": "com.shs.smarthotel",
        "googleMapsApiKey": "AIzaSyCh0dw3B5TMHQFgUUPjXCrvBM2y9pPyBJc"
      },
      "permissions": [
        "android.permission.NFC"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "react-native-nfc-manager"
    ],
    "extra": {
      "eas": {
        "projectId": "d783117e-aa60-474e-845d-6c39fd54283e"
      }
